<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <ImageView
        android:id="@+id/imageView3"
        android:layout_width="60dp"
        android:layout_height="60dp"
        android:layout_marginLeft="260dp"
        android:layout_marginTop="210dp"
        android:layout_weight="0"
        app:srcCompat="@drawable/ic_user" />

    <TextView
        android:id="@+id/textView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_weight="0"
        android:text="TextView" />

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_marginLeft="460dp"
        android:layout_marginRight="100dp"
        android:layout_marginTop="200dp"
        android:layout_weight="2">


        <com.example.android.mmmrkn.presentation.teacher.TeacherCardRecyclerView
            android:id="@+id/teacher_recycle"
            android:layout_width="match_parent"
            android:layout_height="match_parent"></com.example.android.mmmrkn.presentation.teacher.TeacherCardRecyclerView>
    </LinearLayout>

</LinearLayout>
