<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
              xmlns:tools="http://schemas.android.com/tools"
              android:id="@+id/sumsu"
              android:layout_width="match_parent"
              android:layout_height="match_parent"
              android:orientation="horizontal"
              tools:context="com.example.android.mmmrkn.presentation.gohome.GoHomeActivity">

    <fragment
        android:id="@+id/qr_fragment"
        android:name="com.example.android.mmmrkn.presentation.qr.QRFragment"
        android:layout_width="0dp"
        android:layout_height="match_parent"
        android:layout_weight="0.35"
        tools:layout="@layout/fragment_qr"/>

    <fragment
        android:id="@+id/gohome_fragment"
        android:name="com.example.android.mmmrkn.presentation.gohome.GoHomeFragment"
        android:layout_width="0dp"
        android:layout_height="match_parent"
        android:layout_weight="0.65"
        tools:layout="@layout/fragment_go_home" />

</LinearLayout>